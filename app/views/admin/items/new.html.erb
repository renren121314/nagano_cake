<h1>商品新規登録</h1>
<%= form_with model: @item do |f| %>
  <h2>商品画像</h2>
    <%#= f.file_field image, accept: "image/*" %>
  <h2>商品名</h2>
    <%= f.text_field :name %>
  <h2>商品説明</h2>
    <%= f.text_area :introduction %>
  <h2>税抜価格</h2>
    <%#= f.nunber_field :price%>
  <h2>販売ステータス</h2>
    <%= f.select :is_active ,[['販売中',true],['販売停止中',false]] %>
<% end %>